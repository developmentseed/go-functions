{
    "$schema": "http://json.schemastore.org/proxies",
    "proxies": {
        "goindex": {
            "matchCondition": {
                "route": "/",
                "methods": [
                    "GET"
                ]
            },
            "backendUri": "https://ifrcgofrontend.blob.core.windows.net/site/index.html"
        },
        "gopages": {
            "matchCondition": {
                "route": "/{*path}",
                "methods": [
                    "GET"
                ]
            },
            "backendUri": "https://ifrcgofrontend.blob.core.windows.net/site/index.html"
        },
        "gocatchall": {
            "matchCondition": {
                "methods": [
                    "GET"
                ],
                "route": "/assets/{*restOfPath}"
            },
            "backendUri": "https://ifrcgofrontend.blob.core.windows.net/site/assets/{restOfPath}"
        }
    }
}
